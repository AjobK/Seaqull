{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        },
        "compilerOptions": {
            "experimentalDecorators": true
        }
    },
    "plugins": [
        "react"
    ],
    "extends": ["plugin:react/recommended"],
    "rules": {
        "comma-dangle": 0,
        "react/jsx-uses-vars": 0,
        "react/display-name": 0,
        "no-unused-vars": 0,
        "no-console": 0,
        "no-unexpected-multiline": 0,
        "react/prop-types": 0,
        "constructor-super": 0,
        "for-direction": 0,
        "getter-return": 0,
        "no-case-declarations": 0,
        "no-class-assign": 0,
        "no-compare-neg-zero": 0,
        "no-cond-assign": 0,
        "no-const-assign": 0,
        "no-constant-condition": 0,
        "no-control-regex": 0,
        "no-debugger": 0,
        "no-delete-var": 0,
        "no-dupe-args": 0,
        "no-dupe-class-members": 0,
        "no-dupe-keys": 0,
        "no-duplicate-case": 0,
        "no-empty": 0,
        "no-empty-character-class": 0,
        "no-empty-pattern": 0,
        "no-ex-assign": 0,
        "no-extra-boolean-cast": 0,
        "no-extra-semi": 0,
        "no-fallthrough": 0,
        "no-func-assign": 0,
        "no-global-assign": 0,
        "no-inner-declarations": 0,
        "no-invalid-regexp": 0,
        "no-irregular-whitespace": 0,
        "no-mixed-spaces-and-tabs": 0,
        "no-new-symbol": 0,
        "no-obj-calls": 0,
        "no-octal": 0,
        "no-multi-spaces": 0,
        "no-redeclare": 0,
        "no-regex-spaces": 0,
        "no-trailing-spaces": 0,
        "no-self-assign": 0,
        "no-sparse-arrays": 0,
        "no-this-before-super": 0,
        "no-undef": 0,
        "no-unreachable": 0,
        "no-unsafe-finally": 0,
        "no-unsafe-negation": 0,
        "no-unused-labels": 0,
        "no-useless-escape": 0,
        "require-yield": 0,
        "use-isnan": 0,
        "valid-typeof": 0,
        "array-bracket-spacing": 0,
        "array-bracket-newline": 0,
        "jsx-quotes": [0, "prefer-single"],
        "newline-before-return": 0,
        "newline-after-var": 0,
        "no-empty-function": 0,
        "no-duplicate-imports": 0,
        "no-useless-constructor": 0,
        "no-var": 0,
        "object-curly-spacing": [0, "always"],
        "quotes": [0, "single"]
    },
    "settings": {
        "react": {
            "pragma": "React",
            "version": "15.6.1"
        }
    },
    "env": {
        "browser": true,
        "amd": true,
        "node": true,
        "jest": true,
        "mongo": true,
        "es6": true
    },
    "globals": {
        "document": true,
        "node": true
      }
}
